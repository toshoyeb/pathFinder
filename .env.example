# Environment variables for PathFinder app
# Copy this file to .env and replace with your actual keys

# Google Maps API Keys
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY_ALTERNATE=your_alternate_key_here

# Firebase Configuration (these will be public in the built app)
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_PROJECT_NUMBER=your_project_number
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_storage_bucket
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_ANDROID_CLIENT_ID=your_android_client_id
EXPO_PUBLIC_FIREBASE_WEB_CLIENT_ID=your_web_client_id
